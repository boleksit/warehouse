<Window x:Class="WPF_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_App"
        mc:Ignorable="d" FontSize="18" FontFamily="Se"
        Title="Warehouse" Height="450" Width="700">
    <StackPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Package status" Click="bntPackageStatus_OnClick"/>
            <MenuItem Header="Login" Click="bntLogin_OnClick"/>
            <MenuItem Header="DetailPage" Click="bntDetailPage_OnClick"/>
        </Menu>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden" Panel.ZIndex="1" >
        
    </Frame>
    </StackPanel>
</Window>
